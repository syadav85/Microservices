{
  "name": "seneca-entity",
  "description": "Entity plugin for seneca",
  "keywords": [
    "seneca",
    "entity",
    "plugin"
  ],
  "version": "1.3.0",
  "license": "MIT",
  "homepage": "http://senecajs.org",
  "author": "Richard Rodger (http://richardrodger.com/)",
  "main": "entity.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "entity.js"
  ],
  "repository": {
