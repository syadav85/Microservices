  },
  "devDependencies": {
    "async": "2.0.x",
    "code": "3.0.x",
    "coveralls": "2.11.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "eslint-plugin-standard": "2.x.x",
    "gex": "0.2.2",
    "joi": "9.0.x",
    "pre-commit": "1.1.x",
    "lab": "11.0.x",
    "seneca": "plugin",
    "seneca-basic": "0.5.x"
